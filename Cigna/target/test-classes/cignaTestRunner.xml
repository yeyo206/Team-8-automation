<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Cigna Automation" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="60.0"/>
    <parameter name="url" value="https://www.cigna.com/"/>

    <test name="Cigna">
        <classes>
            <class name="CignaTest.CignaTestsRightTopButtons"></class>
            <class name="CignaTest.TestIndividualsAndFamilyBar"></class>
            <class name="CignaTest.TestsUnderstandingInsuranceList"></class>
            <class name="CignaTest.TestUnderstandingInsuranceList2"></class>
            <class name="CignaTest.TestHealthAndWellnessListButtons"></class>
            <class name="CignaTest.TestHealthAndWellnessListButtonList"></class>
            <class name="CignaTest.TestEmployersAndBrokers"></class>
            <class name="CignaTest.TestPlanAndServicesFromEmployers"></class>
            <class name="CignaTest.TestPlanAndServicesFromEmployersExtension"></class>
            <class name="CignaTest.TestPlansAndServicesUsingSupplemental"></class>
            <class name="DataDriven.DataDrivenTests"></class>

            
        </classes>
